{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Account Management System",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/accounting/index.js",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/src/accounting"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Account Management System",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/accounting/index.js",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/src/accounting",
      "stopOnEntry": false,
      "trace": true
    }
  ]
}
